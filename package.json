{
  "name": "d8x-app",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "analyze": "vite-bundle-visualizer",
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "eslint": "eslint src --ext \".js,.jsx,.ts,.tsx\"",
    "eslint-fix": "eslint src --ext \".js,.jsx,.ts,.tsx\" --fix",
    "stylelint": "npx stylelint \"src/**/*.scss\" --color",
    "stylelint-fix": "npx stylelint \"src/**/*.scss\" --color --fix",
    "lint:staged": "lint-staged",
    "lint:ts": "npm run eslint",
    "lint:format": "prettier --check './{src,cypress}/**/*.{ts,tsx}'",
    "lint:format:fix": "prettier --write './{src,cypress}/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "npm run stylelint",
    "lint": "tsc && npm run lint:ts && npm run lint:css",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npm run stylelint"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run prettify",
      "npm run eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cedelabs/widgets-universal": "1.6.0",
    "@d8x/perpetuals-sdk": "2.4.8",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@ethersproject/providers": "5.7.2",
    "@mui/icons-material": "6.2.1",
    "@mui/material": "6.2.1",
    "@mui/utils": "6.2.1",
    "@noble/secp256k1": "2.1.0",
    "@rainbow-me/rainbowkit": "2.2.1",
    "@splidejs/react-splide": "0.7.12",
    "@tanstack/react-query": "5.62.8",
    "@wagmi/core": "2.16.0",
    "@web3auth/auth-adapter": "9.5.1",
    "@web3auth/base": "9.5.1",
    "@web3auth/default-evm-adapter": "9.5.1",
    "@web3auth/ethereum-provider": "9.5.1",
    "@web3auth/no-modal": "9.5.1",
    "@web3auth/web3auth-wagmi-connector": "7.0.0",
    "buffer": "6.0.3",
    "classnames": "2.5.1",
    "core-js": "3.39.0",
    "crypto-js": "4.2.0",
    "cryptocurrency-icons": "0.18.1",
    "date-fns": "4.1.0",
    "firebase": "11.1.0",
    "geonames.js": "3.0.6",
    "html-to-image": "1.11.11",
    "i18next": "24.1.2",
    "i18next-browser-languagedetector": "8.0.2",
    "i18next-http-backend": "3.0.1",
    "jotai": "2.10.4",
    "lightweight-charts": "4.2.2",
    "lightweight-charts-react-wrapper": "2.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.4",
    "react-i18next": "14.1.0",
    "react-minimal-pie-chart": "8.4.0",
    "react-resize-detector": "10.0.1",
    "react-router-dom": "6.22.3",
    "react-secure-storage": "1.3.2",
    "react-toastify": "11.0.1",
    "react-world-flags": "1.6.0",
    "viem": "2.21.55",
    "wagmi": "2.14.3"
  },
  "devDependencies": {
    "@types/crypto-js": "4.2.2",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/react-world-flags": "1.6.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "sass": "1.83.0",
    "stylelint": "16.12.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.10.0",
    "typescript": "5.7.2",
    "vite": "5.2.10",
    "vite-bundle-visualizer": "1.1.0",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-node-polyfills": "0.21.0",
    "vite-plugin-stylelint": "5.3.1",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2"
  }
}
